<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8"/><meta charset="UTF-8">
    <title>
      <% if (title) { %>
        <%= title %>
      <% } else { %>
        Oligrapher Demo
      <% } %>
    </title>
    <% if (title) { %>
      <meta property="og:title" content="<%= title %>">
    <% } %>
    <% if (image) { %>
      <meta property="og:image" content="<%= image %>">
    <% } %>
    <% if (description) { %>
      <meta property="og:description" content="<%= description %>">
    <% } %>
    <link href="<%= env == 'development' ? '' : '/react' %>/oligrapher.css" rel="stylesheet" />
    <style>
      body {
        margin: 0;
      }

      #screen {
        background-color: rgba(192, 192, 192, 0.5);
        z-index: 10;
        position: absolute;
        width: 100%;
        height: 100%;
        display: none;
      }

      .editForm {
        display: none;
        background: white;
        width: 400px;
        padding: 20px;
        border-radius: 5px;
        position: absolute;
        z-index: 20;
        top: 300px;
        left: 0;
        right: 0;
        margin: 0 auto;
      }

      .editForm h3 {
        margin-top: 0;
      }

      input, select {
        width: 300px;
      }
    </style>

  </head>

  <body>
    <div id="screen"></div>
    <div id="oligrapher">
    </div>      

    <button id="zoomIn">zoom in</button>
    <button id="zoomOut">zoom out</button>   
    <br>
    <br>

    <button id="import">import</button>
    <button id="export">export</button>
    <br>    
    <textarea id="data" rows="10" cols="80"></textarea>

    <div id="addNode" class="editForm">
      <h3>Add Node</h3>
      <input type="text" id="addNodeName" placeholder="name" /><br>
      <button id="addNodeButton">Add</button>
    </div>

    <div id="addEdge" class="editForm">
      <h3>Add Edge</h3>
      <select id="addEdgeNode1Id"></select><br>
      <select id="addEdgeNode2Id"></select><br>
      <input id="addEdgeLabel" type="text" placeholder="label" /><br>
      <button id="addEdgeButton">Add</button>
    </div>

    <div id="updateNode" class="editForm">
      <h3>Update Node</h3>
      <input type="text" id="updateNodeName" placeholder="name" /><br>
      <input type="text" id="updateNodeImage" placeholder="image URL" /><br>
      <select id="updateNodeScale">
        <option value="">Size</option>
        <option value="1">1x</option>
        <option value="1.5">1.5x</option>
        <option value="2">2x</option>
        <option value="3">3x</option>
      </select><br>
      <button id="updateNodeButton">Update</button>
    </div>

    <% if (env == 'development') { %>
      <script src="http://localhost:8090/webpack-dev-server.js"></script>
      <script src="http://localhost:8090/build/vendors.js"></script>
      <script src="http://localhost:8090/build/oligrapher.js"></script>
      <script src="http://localhost:8080/js/LsDataConverter.js"></script>
      <script src="http://localhost:8080/js/PopoloDataConverter.js"></script>
    <% } else { %>
      <script src="/oligrapher.min.js"></script>
      <script src="/LsDataConverter.js"></script>
    <% } %>
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>

    <script>
      var oli;

      function getParam(name) {
        var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
        return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
      }

      function updateNodeSelects() {
        var hash = oli.export().nodes;
        var nodes = Object.keys(hash).map(function(key) { return hash[key]; })
          .sort(function(a, b) { return a.display.name.localeCompare(b.display.name); });

        var select1 = $('#addEdgeNode1Id');
        var select2 = $('#addEdgeNode2Id');

        select1.html("<option value=''>Node 1</option>");
        select2.html("<option value=''>Node 2</option>");


        nodes.forEach(function(node) {
          var option = document.createElement("option");
          option.setAttribute("value", node.id);
          option.innerHTML = node.display.name;
          clone = option.cloneNode(true);
          select1[0].appendChild(option);
          select2[0].appendChild(clone);
        });
      }

      function toggleForm(selector) {
        var newDisplay = $(selector).css('display') == 'block' ? 'none' : 'block';
        $('.editForm').hide();
        $('#screen').css('display', newDisplay);
        $(selector).css('display', newDisplay);
      }

      var keys = {};

      $(window).on('keydown', function(e) {
        keys[e.keyCode] = true;
        var selection = oli.getSelection();

        // ALT+N
        if (keys[18] && keys[78]) {
          if (selection.nodeIds.length == 1) {
            toggleForm('#updateNode');
          } else if (selection.nodeIds.length == 0) {
            toggleForm('#addNode');
          }
        }

        // ALT+E
