        var transformMapData = function(data) {
          var nodes =  data.entities.map(function(e) {
            return {
              id: e.id,
              display: { name: e.name }
            };
          });

          var edges = data.rels.map(function(r) {
            return {
              id: r.id,
              n1: r.entity1_id,
              n2: r.entity2_id,
              display: { label: r.label }
            }
          });

          return {
            id: data.id,
            title: data.title,
            nodes: nodes,
            edges: edges
          };
        };

        var transformCollectionData = function(data) {
          return {
            id: data.id,
            title: data.title,
            graphs: data.maps.map(function(map) { 
              return transformMapData(map); 
            })
          };
        };

        data = transformCollectionData(data);
        main.run(data);
