          if (selection.edgeIds.length == 1) {
            toggleForm('#updateEdge');
          } else if (selection.edgeIds.length == 0) {
            toggleForm('#addEdge');
          }
        }
      });

      $(window).on('keyup', function(e) {
        delete keys[e.keyCode];
      });

