{
  "name": "oligrapher2",
  "version": "0.3.0",
  "description": "network graph visualizer",
  "main": "app/main.jsx",
  "scripts": {
    "dev": "webpack-dev-server --color --progress --config webpack.dev.js",
    "fake-news-server": "http-server ./build -p 8091",
    "build": "webpack -p --display-modules --config webpack.prod.js",
    "dev-package": "cat build/oligrapher.js build/LsDataSource.js build/LsDataConverter.js > build/oligrapher-dev.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skomputer/oligrapher2.git"
  },
  "author": "skomputer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/skomputer/oligrapher2/issues"
  },
  "homepage": "https://github.com/skomputer/oligrapher2/",
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-anything-sortable": "^1.7.2",
    "react-color": "^2.11.7",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^15.6.2",
    "react-draggable": "^2.2.6",
    "react-hotkeys": "^0.10.0",
    "react-medium-editor": "^1.8.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "redux-undo": "^0.6.0",
    "shortid": "^2.2.8",
    "springy": "^2.7.1",
    "titleize": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^2.8.2",
    "file-loader": "^1.1.6",
    "http-server": "^0.11.1",
    "jest-cli": "^20.0.3",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.2.7",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^2.10.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/app/styles/test/styleMock.js"
    },
    "testRegex": "(/__tests__/(?!support).*|\\.(test|spec))\\.jsx?$"
  }
}
