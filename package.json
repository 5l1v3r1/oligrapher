{
  "name": "show-me-the-money",
  "version": "1.0.0",
  "description": "learning react w/ little sis",
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  },
  "main": "app/main.jsx",
  "scripts": {
    "build-css": "sass ./styles/_main.scss ./build/main.css",
    "dev": "npm run dev-serve | npm run dev-build",
    "dev-build": "npm run build-css && webpack-dev-server --devtool eval --progress --history-api-fallback --colors --hot --content-base build --port 8090 --config webpack.dev.config",
    "dev-serve": "NODE_ENV=development node app/server.jsx",
    "prod": "npm run prod-build && npm run prod-serve",
    "prod-build-nomin": "NODE_ENV=production webpack --config webpack.prod.config.js && npm run build-css",
    "prod-build": "NODE_ENV=production webpack -p --config webpack.prod.config.js && npm run build-css",
    "prod-serve": "NODE_ENV=production node app/server.jsx",
    "test": "mocha --recursive --compilers js:babel/register",
    "debug-test": "mocha --debug --recursive --compilers js:babel/register",
    "jtest": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aguestuser/show-me-the-money.git"
  },
  "author": "aguestuser",
  "license": "gpl",
  "bugs": {
    "url": "https://github.com/aguestuser/show-me-the-money/issues"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "superagent"
    ]
  },
  "homepage": "https://github.com/aguestuser/show-me-the-money/",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.3",
    "bootstrap": "^3.3.4",
    "bootstrap-sass": "^3.3.4",
    "bootstrap-sass-loader": "^1.0.4",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "classnames": "^2.1.2",
    "css-loader": "^0.13.1",
    "ejs": "^2.3.4",
    "expose-loader": "^0.7.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.3",
    "html-webpack-plugin": "^1.4.0",
    "http-server": "^0.8.0",
    "image-webpack-loader": "^1.4.0",
    "imagemin": "^3.2.0",
    "immutable": "^3.7.5",
    "imports-loader": "^0.6.3",
    "jarr.js": "^0.1.2",
    "jest-cli": "^0.4.5",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "lodash": "^3.9.1",
    "mocha": "^2.2.5",
    "node-jsx": "^0.13.3",
    "node-libs-browser": "^0.5.0",
    "node-sass": "^3.1.2",
    "pushstate-server": "^1.6.0",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.2",
    "react-draggable": "^1.0.1",
    "react-hot-loader": "^1.2.7",
    "react-redux": "^4.0.0",
    "react-tools": "^0.13.3",
    "redux": "^3.0.4",
    "redux-immutablejs": "0.0.7",
    "rimraf": "^2.4.3",
    "sass": "^0.5.0",
    "sass-loader": "^1.0.2",
    "semver": "^5.0.3",
    "serve": "^1.4.0",
    "striptags": "^2.0.3",
    "style-loader": "^0.12.2",
    "superagent": "^1.2.0",
    "superagent-jsonp": "0.0.2",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2",
    "xml-loader": "^1.0.0"
  }
}
