{
  "name": "oligrapher2",
  "version": "0.1.0",
  "description": "network graph visualizer",
  "main": "build/oligrapher.min.js"
  "scripts": {
    "dev-build": "webpack-dev-server --devtool eval --progress --history-api-fallback --colors --hot --content-base build --port 8090 --config webpack.dev.config",
    "prod-build": "NODE_ENV=production webpack -p --display-modules --config webpack.prod.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skomputer/oligrapher2.git"
  },
  "author": "skomputer",
  "license": "gpl",
  "bugs": {
    "url": "https://github.com/skomputer/oligrapher2/issues"
  },
  "homepage": "https://github.com/skomputer/oligrapher2/",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "classnames": "^2.2.0",
    "lodash": "^3.10.1",
    "pushstate-server": "^1.6.0",
    "react": "^0.14",
    "react-draggable": "^1.0.1",
    "react-hotkeys": "^0.6.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "shortid": "^2.2.4",
    "springy": "^2.7.1"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "jasmine-expect": "^1.22.3",
    "jest-cli": "^0.7.1",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.2.7",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/shortid",
      "<rootDir>/node_modules/react-draggable"
    ]
  }
}
