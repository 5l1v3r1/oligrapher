{
  "name": "oligrapher2",
  "version": "0.3.9",
  "description": "network graph visualizer",
  "main": "app/main.jsx",
  "scripts": {
    "dev": "webpack-dev-server --color --progress --config webpack.dev.js",
    "fake-news-server": "http-server ./build -p 8091",
    "build": "webpack -p --display-modules --config webpack.prod.js",
    "dev-package": "cat build/oligrapher.js build/LsDataSource.js build/LsDataConverter.js > build/oligrapher-dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "profile": "webpack --profile --display-modules --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/public-accountability/oligrapher"
  },
  "author": "skomputer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/public-accountability/oligrapher/issues"
  },
  "homepage": "https://littlesis.org/oligrapher",
  "dependencies": {
    "@githubprimer/octicons-react": "^8.3.0",
    "classnames": "^2.2.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-anything-sortable": "^1.7.4",
    "react-color": "^2.17.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^16.7.0",
    "react-draggable": "^3.1.1",
    "react-hotkeys": "^1.1.4",
    "react-medium-editor": "^1.8.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^0.6.0",
    "regenerator-runtime": "^0.13.1",
    "shortid": "^2.2.14",
    "springy": "^2.8.0",
    "titleize": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.23.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "file-loader": "^2.0.0",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.7.0",
    "redux-logger": "^3.0.6",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/app/styles/test/styleMock.js"
    },
    "roots": [
      "test"
    ],
    "testRegex": "(.*)\\.test\\.jsx?$"
  }
}
