{
  "name": "show-me-the-money",
  "version": "1.0.0",
  "description": "learning react w/ little sis",
  "engines": {
    "node": "0.10.x"
  },
  "main": "app/main.jsx",
  "scripts": {
    "dev": "npm run dev-serve | npm run dev-build",
    "dev-build": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --port 8090 --config webpack.dev.config",
    "dev-serve": "http-server -p 8080 ./dev",
    "prod": "npm run prod-build && npm run prod-serve",
    "prod-build": "NODE_ENV=production webpack -p --config webpack.prod.config.js",
    "prod-serve": "http-server -p $PORT ./build",
    "test": "mocha --compilers js:babel/register",
    "jtest": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aguestuser/show-me-the-money.git"
  },
  "author": "aguestuser",
  "license": "gpl",
  "bugs": {
    "url": "https://github.com/aguestuser/show-me-the-money/issues"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "superagent"
    ]
  },
  "homepage": "https://github.com/aguestuser/show-me-the-money/",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.3",
    "bootstrap": "^3.3.4",
    "bootstrap-sass": "^3.3.4",
    "bootstrap-sass-loader": "^1.0.4",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "css-loader": "^0.13.1",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.3",
    "html-webpack-plugin": "^1.4.0",
    "http-server": "^0.8.0",
    "immutable": "^3.7.3",
    "imports-loader": "^0.6.3",
    "jest-cli": "^0.4.5",
    "jquery": "^2.1.4",
    "jsx-loader": "^0.13.2",
    "lodash": "^3.9.1",
    "marty": "^0.10.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.0",
    "node-sass": "^3.1.2",
    "react": "^0.13.3",
    "react-bootstrap": "^0.22.6",
    "react-draggable": "^0.8.0",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "sass": "^0.5.0",
    "sass-loader": "^1.0.2",
    "serve": "^1.4.0",
    "style-loader": "^0.12.2",
    "superagent": "^1.2.0",
    "superagent-jsonp": "0.0.2",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2"
  }
}
