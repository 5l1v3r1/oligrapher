{
  "name": "oligrapher2",
  "version": "1.0.0",
  "description": "network graph visualizer",
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  },
  "main": "app/main.jsx",
  "scripts": {
    "dev": "npm run dev-serve | npm run dev-build",
    "dev-build": "npm run && webpack-dev-server --devtool eval --progress --history-api-fallback --colors --hot --content-base build --port 8090 --config webpack.dev.config",
    "dev-serve": "NODE_ENV=development node app/server.jsx",
    "prod": "npm run prod-build && npm run prod-serve",
    "prod-build-nomin": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "prod-build": "NODE_ENV=production webpack -p --display-modules --config webpack.prod.config.js",
    "prod-serve": "NODE_ENV=production node app/server.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aguestuser/show-me-the-money.git"
  },
  "author": "aguestuser",
  "license": "gpl",
  "bugs": {
    "url": "https://github.com/aguestuser/show-me-the-money/issues"
  },
  "homepage": "https://github.com/skomputer/oligrapher2/",
  "dependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "css-loader": "^0.13.1",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "file-loader": "^0.8.3",
    "html-webpack-plugin": "^1.4.0",
    "http-server": "^0.8.0",
    "image-webpack-loader": "^1.4.0",
    "imports-loader": "^0.6.3",
    "jsx-loader": "^0.13.2",
    "lodash": "^3.10.1",
    "node-jsx": "^0.13.3",
    "node-libs-browser": "^0.5.0",
    "pushstate-server": "^1.6.0",
    "react": "^0.14.0",
    "react-draggable": "^1.0.1",
    "react-hot-loader": "^1.2.7",
    "react-hotkeys": "^0.6.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "semver": "^5.0.3",
    "serve": "^1.4.0",
    "shortid": "^2.2.4",
    "springy": "^2.7.1",
    "style-loader": "^0.12.2",
    "superagent": "^1.2.0",
    "superagent-jsonp": "0.0.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2",
    "xml-loader": "^1.0.0"
  }
}
